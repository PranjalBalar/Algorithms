# -*- coding: utf-8 -*-

#This program creates a modified version of a hash table to store values using chaining

l = [None] * 10

class Node:  
    def __init__(self, obj, value):
    	self.value = value
        self.item = obj
        self.next_node = None
        self.prev_node = None

class HashTable():

	def __init__(self, lst):
		self.lst = lst
		self.len = len(self.lst)

	def add(self, x, value):
		n = hash(x) % self.len
		if self.lst[n] == None:
			self.lst[n] = Node(x, value)
			return self.lst
		else:
			a = self.check_add(self.lst[n])
			a.next_node = Node(x, value)	
			return self.lst

	def check_add(self, node):
		if node.next_node == None:
			return node
		else:
			return self.check_add(node.next_node)

	def get(self, x):
		n = hash(x) % self.len
		if self.lst[n].item == x:
			return self.lst[n].value
		else:
			a = self.check_get(x, self.lst[n])
			if a != None:
				return a.value
			else:
				return None

	def check_get(self, x, node):
		if node.next_node == None:
			return None
		elif node.next_node.item == x:
			return node.next_node
		else:
			return self.check_get(node.next_node)

h = HashTable(l)