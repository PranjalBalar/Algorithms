# -*- coding: utf-8 -*-

#This program builds a stack using an array
#There are 3 functions: Push, pop and peek
#The complexity of all the functions is O(1)
class Stack:
    def __init__ (self):
        self.a = []
        self.index = 0
        
    def push(self, x):
        self.a.append(x)
        self.index += 1
        return self.a
    
    def pop(self):
        self.index -= 1
        return self.a.pop()

    def peek(self):
        return self.a[-1]

stack1 = Stack()
